{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "singularName": "todo",
    "pluralName": "todos",
    "displayName": "todo"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titel": {
      "type": "string"
    },
    "due": {
      "type": "date"
    },
    "priority": {
      "type": "enumeration",
      "default": "regular",
      "enum": [
        "minor",
        "regular",
        "critical"
      ]
    },
    "done": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "todos"
    }
  }
}
